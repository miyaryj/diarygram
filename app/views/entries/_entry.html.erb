<div class="list-group-item">
  <%= link_to '#', class: 'link-gentle' do %>
    <div class="row-action-primary">
      <%= image_tag(entry.thumbnail_url, class: 'timeline-thumbnail') %>
    </div>
    <div class="row-content">
      <h4 class="list-group-item-heading"><%= entry.text %></h4>
      <p class="list-group-item-text non-feedback"><%= entry.date %></p>
      <p class="list-group-item-text non-feedback"><%= entry.user.email %></p>
    </div>
  <% end %>
</div>
<div class="list-group-separator"></div>
